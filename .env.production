# 生产环境配置
NODE_ENV=production
PORT=3000
APP_NAME=Backend Admin
APP_VERSION=1.0.0

# 数据库配置 - Docker Compose 环境配置
DATABASE_HOST=mysql
DATABASE_PORT=3306
DATABASE_USERNAME=admin
DATABASE_PASSWORD=admin123
DATABASE_NAME=backend_admin

# JWT配置 - 生产环境必须使用强密钥
JWT_SECRET=your-very-strong-production-secret-key-change-this
JWT_EXPIRES_IN=24h

# Redis配置 - Docker Compose 环境配置
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=redis123

# 日志配置
LOG_LEVEL=info