# 生产环境配置
NODE_ENV=production
PORT=3000
APP_NAME=Backend Admin
APP_VERSION=1.0.0

# 数据库配置 - 生产环境请使用实际的数据库配置
DATABASE_HOST=your-production-db-host
DATABASE_PORT=5432
DATABASE_USERNAME=your-production-db-user
DATABASE_PASSWORD=your-production-db-password
DATABASE_NAME=backend_admin_prod

# JWT配置 - 生产环境必须使用强密钥
JWT_SECRET=your-very-strong-production-secret-key-change-this
JWT_EXPIRES_IN=24h

# Redis配置 - 生产环境配置
REDIS_HOST=your-production-redis-host
REDIS_PORT=6379
REDIS_PASSWORD=your-production-redis-password

# 日志配置
LOG_LEVEL=info